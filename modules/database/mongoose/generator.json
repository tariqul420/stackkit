{
  "name": "mongoose",
  "type": "database",
  "priority": 5,
  "operations": [
    {
      "type": "create-file",
      "source": "lib/db.ts",
      "destination": "src/lib/db.ts"
    },
    {
      "type": "create-file",
      "source": "models/User.ts",
      "destination": "src/lib/models.ts"
    }
  ],
  "dependencies": {
    "mongoose": "^8.8.0"
  },
  "devDependencies": {},
  "envVars": {
    "MONGODB_URI": "mongodb://localhost:27017/database_name"
  }
}