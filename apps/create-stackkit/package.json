{
  "name": "create-stackkit-app",
  "version": "0.4.3",
  "description": "Create a new StackKit project with modular composition",
  "main": "dist/index.js",
  "bin": {
    "create-stackkit": "./bin/create-stackkit.js"
  },
  "type": "commonjs",
  "files": [
    "dist",
    "bin",
    "templates",
    "modules"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tariqul420/stackkit.git",
    "directory": "apps/create-stackkit"
  },
  "homepage": "https://github.com/tariqul420/stackkit#readme",
  "bugs": {
    "url": "https://github.com/tariqul420/stackkit/issues"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "npm run clean && tsc && npm run copy-assets",
    "copy-assets": "cp -r ../../templates . && cp -r ../../modules .",
    "clean": "rm -rf dist templates modules",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "create",
    "stackkit",
    "generator",
    "nextjs",
    "template"
  ],
  "author": "Tariqul Islam",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.3.8",
    "ora": "^5.4.1",
    "validate-npm-package-name": "^5.0.1",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@babel/preset-react": "^7.28.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.0",
    "@types/validate-npm-package-name": "^4.0.2",
    "typescript": "^5.3.3",
    "recast": "^0.20.5",
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-typescript": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/plugin-transform-react-jsx": "^7.27.1"
  }
}